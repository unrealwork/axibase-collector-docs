<list>
  <com.axibase.collector.model.job.json.JsonJobConfig>
    <name>BLS</name>
    <cronExpression>0 0/15 * * * ?</cronExpression>
    <enabled>false</enabled>
    <jobType>JSON</jobType>
    <defaultTags/>
    <checkLastTime>false</checkLastTime>
    <commitInterval>10</commitInterval>
    <errorThreshold>0</errorThreshold>
    <configurations>
      <com.axibase.collector.model.job.json.JsonConfiguration>
        <name>BLS</name>
        <protocol>HTTP</protocol>
        <path>http://api.bls.gov/publicAPI/v2/timeseries/data/</path>
        <format>JSON</format>
        <payload>{&quot;seriesid&quot;: [&quot;CUSR0000SA0&quot;]}</payload>
        <payloadMethod>POST</payloadMethod>
        <httpHeaders>
          <entry>
            <string>Content-Type</string>
            <string>application/json</string>
          </entry>
        </httpHeaders>
        <enableWebDriver>false</enableWebDriver>
        <driverTimeout>0</driverTimeout>
        <deleteFileOnUpload>false</deleteFileOnUpload>
        <ignoreDuplicates>false</ignoreDuplicates>
        <settings>
          <com.axibase.collector.model.job.json.JsonConfigurationSetting>
            <jsonPath>$.Results.series.*.data.*</jsonPath>
            <depth>1</depth>
            <defaultEntity>bls</defaultEntity>
            <customTags/>
            <renamedFields/>
            <timeEnabled>true</timeEnabled>
            <timeField>year+${&quot;period&quot;?keep_after(&quot;M&quot;)}</timeField>
            <timeFormat>yyyy+MM</timeFormat>
            <timeZone>UTC</timeZone>
            <seriesEnabled>true</seriesEnabled>
            <includedFields>
              <string>*</string>
            </includedFields>
            <excludedFields>
              <string>value</string>
              <string>periodName</string>
            </excludedFields>
            <metricNameField>$.Results.series[{PARENT(2)}].seriesID</metricNameField>
            <metricValueField>value</metricValueField>
            <propertyEnabled>false</propertyEnabled>
            <propertyKeyFields/>
            <propertyValueFields/>
            <messageEnabled>false</messageEnabled>
            <messageTagFields/>
          </com.axibase.collector.model.job.json.JsonConfigurationSetting>
        </settings>
      </com.axibase.collector.model.job.json.JsonConfiguration>
    </configurations>
  </com.axibase.collector.model.job.json.JsonJobConfig>
</list>