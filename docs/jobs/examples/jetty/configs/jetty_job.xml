<list>
    <com.axibase.collector.model.job.jmx.JmxJobConfig>
        <name>jmx-jetty/name>
        <cronExpression>45 * * * * ?</cronExpression>
        <enabled>true</enabled>
        <jobType>JMX</jobType>
        <defaultTags/>
        <checkLastTime>false</checkLastTime>
        <commitInterval>10</commitInterval>
        <errorThreshold>0</errorThreshold>
        <configurations>
            <com.axibase.collector.model.job.jmx.JmxConfiguration>
                <name>jetty-properties</name>
                <host>jetty_host</host>
                <port>1099</port>
                <userName>axibase</userName>
                <password>password</password>
                <trackingMBeans>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.eclipse.jetty.server.session:context=*,id=*,type=hashsessionmanager</objectName>
                        <attributes>*</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.eclipse.jetty.server:id=*,type=server</objectName>
                        <attributes>*</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                </trackingMBeans>
                <entity>java.lang:type=Runtime&gt;SystemProperties.java.rmi.server.hostname.value</entity>
                <metricPrefix>jmx.</metricPrefix>
                <propertyTypePrefix>jmx.jetty.</propertyTypePrefix>
                <serviceName></serviceName>
                <excludedMetrics/>
                <excludedProperties>
                    <string>ObjectName</string>
                    <string>childHandlers_*</string>
                    <string>connectors_*</string>
                    <string>contexts_*</string>
                </excludedProperties>
                <seriesTags/>
                <putType>PROPERTY</putType>
            </com.axibase.collector.model.job.jmx.JmxConfiguration>
            <com.axibase.collector.model.job.jmx.JmxConfiguration>
                <name>jetty-series</name>
                <host>jetty_host</host>
                <port>1099</port>
                <userName>axibase</userName>
                <password>password</password>
                <trackingMBeans>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.eclipse.jetty.server.session:context=*,id=*,type=hashsessionmanager</objectName>
                        <attributes>sessionTimeMean, sessionTimeMax, sessionTimeStdDev, sessionTimeTotal, sessions,
                            sessionsMax, sessionsTotal
                        </attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                </trackingMBeans>
                <entity>java.lang:type=Runtime&gt;SystemProperties.java.rmi.server.hostname.value</entity>
                <metricPrefix>jmx.jetty.</metricPrefix>
                <propertyTypePrefix>jvm.</propertyTypePrefix>
                <serviceName></serviceName>
                <excludedMetrics/>
                <excludedProperties>
                    <string>ObjectName</string>
                </excludedProperties>
                <seriesTags/>
                <putType>SERIES</putType>
            </com.axibase.collector.model.job.jmx.JmxConfiguration>
        </configurations>
    </com.axibase.collector.model.job.jmx.JmxJobConfig>
</list>