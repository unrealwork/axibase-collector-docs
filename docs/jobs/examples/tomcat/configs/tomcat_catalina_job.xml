<list>
    <com.axibase.collector.model.job.jmx.JmxJobConfig>
        <name>jmx-tomcat</name>
        <cronExpression>0/15 * * * * ?</cronExpression>
        <enabled>true</enabled>
        <jobType>JMX</jobType>
        <defaultTags/>
        <checkLastTime>false</checkLastTime>
        <commitInterval>10</commitInterval>
        <errorThreshold>0</errorThreshold>
        <configurations>
            <com.axibase.collector.model.job.jmx.JmxConfiguration>
                <name>tomcat-series</name>
                <host>tomcat_host</host>
                <port>3090</port>
                <userName>axibase</userName>
                <password>password</password>
                <trackingMBeans>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:host=localhost,path=/,type=Manager</objectName>
                        <attributes>expiredSessions, activeSessions, maxActive, processingTime, sessionAverageAliveTime,
                            sessionCounter, sessionCreateRate, sessionExpireRate, sessionMaxAliveTime
                        </attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:j2eeType=WebModule,name=//localhost/,*</objectName>
                        <attributes>errorCount, minTime, maxTime, requestCount, processingTime</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=GlobalRequestProcessor,name=*http*</objectName>
                        <attributes>bytesReceived, bytesSent, errorCount, maxTime, processingTime, requestCount
                        </attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=ThreadPool,name=*http*</objectName>
                        <attributes>connectionCount, currentThreadCount, currentThreadsBusy, acceptorThreadCount,
                            maxThreads, maxConnections
                        </attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:j2eeType=Servlet,WebModule=//localhost/,name=default,*</objectName>
                        <attributes>processingTime, errorCount, requestCount, maxTime</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:j2eeType=Servlet,WebModule=//localhost/,name=jsp,*</objectName>
                        <attributes>processingTime, errorCount, requestCount, maxTime</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                </trackingMBeans>
                <entity>java.lang:type=Runtime&gt;SystemProperties.java.rmi.server.hostname.value</entity>
                <metricPrefix>jmx.tomcat.</metricPrefix>
                <propertyTypePrefix>jvm.</propertyTypePrefix>
                <serviceName></serviceName>
                <excludedMetrics/>
                <excludedProperties>
                    <string>ObjectName</string>
                </excludedProperties>
                <seriesTags/>
                <putType>SERIES</putType>
            </com.axibase.collector.model.job.jmx.JmxConfiguration>
            <com.axibase.collector.model.job.jmx.JmxConfiguration>
                <name>tomcat-properties</name>
                <host>tomcat_host</host>
                <port>3090</port>
                <userName>axibase</userName>
                <password>password</password>
                <trackingMBeans>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=Server</objectName>
                        <attributes>accessCount</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=Connector,port=*,*</objectName>
                        <attributes>maxHeaderCount</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=Engine</objectName>
                        <attributes>stateName</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=GlobalRequestProcessor,name=*http*</objectName>
                        <attributes>maxTime</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=ProtocolHandler,port=*</objectName>
                        <attributes>algorithm</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:type=ThreadPool,name=*http*</objectName>
                        <attributes>acceptorThreadCount</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>*:j2eeType=WebModule,name=//localhost/,*</objectName>
                        <attributes>override</attributes>
                        <propertyType>WebModule</propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                </trackingMBeans>
                <entity>java.lang:type=Runtime&gt;SystemProperties.java.rmi.server.hostname.value</entity>
                <metricPrefix>jmx.tomcat.</metricPrefix>
                <propertyTypePrefix>jmx.tomcat.</propertyTypePrefix>
                <serviceName></serviceName>
                <excludedMetrics/>
                <excludedProperties>
                    <string>*Xml</string>
                    <string>ObjectName</string>
                    <string>children_*</string>
                    <string>deploymentDescriptor</string>
                    <string>objectName</string>
                    <string>servlets_*</string>
                    <string>valveObjectNames_*</string>
                    <string>welcomeFiles_*</string>
                </excludedProperties>
                <seriesTags/>
                <putType>PROPERTY</putType>
            </com.axibase.collector.model.job.jmx.JmxConfiguration>
        </configurations>
    </com.axibase.collector.model.job.jmx.JmxJobConfig>
</list>