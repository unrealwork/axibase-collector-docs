<list>
    <com.axibase.collector.model.job.jmx.JmxJobConfig>
        <name>jmx-derby</name>
        <cronExpression>0 0/5 * * * ?</cronExpression>
        <enabled>true</enabled>
        <jobType>JMX</jobType>
        <defaultTags/>
        <checkLastTime>false</checkLastTime>
        <commitInterval>10</commitInterval>
        <errorThreshold>0</errorThreshold>
        <configurations>
            <com.axibase.collector.model.job.jmx.JmxConfiguration>
                <name>derby-series</name>
                <host>derby_host</host>
                <port>3090</port>
                <userName>axibase</userName>
                <password>password</password>
                <trackingMBeans>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:db=*,name=*,system=*,type=CacheManager</objectName>
                        <attributes>*</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:system=*,type=NetworkServer</objectName>
                        <attributes>*</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                </trackingMBeans>
                <entity>java.lang:type=Runtime&gt;SystemProperties.java.rmi.server.hostname.value</entity>
                <metricPrefix>jmx.derby.</metricPrefix>
                <propertyTypePrefix>jvm.</propertyTypePrefix>
                <serviceName></serviceName>
                <excludedMetrics>
                    <string>system</string>
                </excludedMetrics>
                <excludedProperties>
                    <string>ObjectName</string>
                </excludedProperties>
                <seriesTags/>
                <putType>SERIES</putType>
            </com.axibase.collector.model.job.jmx.JmxConfiguration>
            <com.axibase.collector.model.job.jmx.JmxConfiguration>
                <name>derby-properties</name>
                <host>derby_host</host>
                <port>3090</port>
                <userName>axibase</userName>
                <password>password/password>
                <trackingMBeans>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:db=*,name=*,system=*,type=CacheManager</objectName>
                        <attributes>AllocatedEntries</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:system=*,type=JDBC</objectName>
                        <attributes>CompliantDriver</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:system=*,type=Management</objectName>
                        <attributes>SystemIdentifier</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:system=*,type=NetworkServer</objectName>
                        <attributes>AccumulatedConnectionCount</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                    <com.axibase.collector.model.job.jmx.TrackingMBean>
                        <objectName>org.apache.derby:jar=*,system=*,type=Version</objectName>
                        <attributes>Alpha</attributes>
                        <propertyType></propertyType>
                    </com.axibase.collector.model.job.jmx.TrackingMBean>
                </trackingMBeans>
                <entity>java.lang:type=Runtime&gt;SystemProperties.java.rmi.server.hostname.value</entity>
                <metricPrefix>jmx.</metricPrefix>
                <propertyTypePrefix>jmx.derby.</propertyTypePrefix>
                <serviceName></serviceName>
                <excludedMetrics/>
                <excludedProperties>
                    <string>system</string>
                </excludedProperties>
                <seriesTags/>
                <putType>PROPERTY</putType>
            </com.axibase.collector.model.job.jmx.JmxConfiguration>
        </configurations>
    </com.axibase.collector.model.job.jmx.JmxJobConfig>
</list>