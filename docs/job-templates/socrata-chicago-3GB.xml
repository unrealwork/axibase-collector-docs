<list>
  <com.axibase.collector.model.job.socrata.SocrataJobConfig>
    <name>socrata-chicago-3GB</name>
    <cronExpression>0 0/15 * * * ?</cronExpression>
    <enabled>false</enabled>
    <jobType>SOCRATA</jobType>
    <defaultTags/>
    <checkLastTime>false</checkLastTime>
    <commitInterval>10</commitInterval>
    <errorThreshold>0</errorThreshold>
    <configurations>
      <com.axibase.collector.model.job.socrata.SocrataConfiguration>
        <name>Chicago Crimes - 2001 to present</name>
        <path>https://data.cityofchicago.org/api/views/ijzp-q8t2/</path>
        <ignoreDuplicates>true</ignoreDuplicates>
        <settings>
          <com.axibase.collector.model.job.socrata.SocrataConfigurationSetting>
            <entityField>$.meta.view.id</entityField>
            <timeEnabled>true</timeEnabled>
            <timeField>date</timeField>
            <timeFormat>yyyy-MM-dd&apos;T&apos;HH:mm:ss</timeFormat>
            <timeZone>UTC</timeZone>
            <seriesEnabled>true</seriesEnabled>
            <metricPrefix>chg.</metricPrefix>
            <includedFields>
              <string>*</string>
            </includedFields>
            <excludedFields>
              <string>beat</string>
              <string>block</string>
              <string>case_number</string>
              <string>community_area</string>
              <string>district</string>
              <string>domestic</string>
              <string>fbi_code</string>
              <string>id</string>
              <string>iucr</string>
              <string>latitude</string>
              <string>longitude</string>
              <string>updated_on</string>
              <string>ward</string>
              <string>x_coordinate</string>
              <string>y_coordinate</string>
              <string>year</string>
            </excludedFields>
            <propertyEnabled>false</propertyEnabled>
            <propertyTypeField>$.meta.view.name</propertyTypeField>
            <propertyKeyFields/>
            <propertyValueFields/>
            <messageEnabled>false</messageEnabled>
            <messageTypeField>$.meta.view.category</messageTypeField>
            <messageSourceField>$.meta.view.attribution</messageSourceField>
            <messageTagFields>
              <string>*</string>
            </messageTagFields>
            <jsonPath>$.data.*</jsonPath>
            <depth>1</depth>
          </com.axibase.collector.model.job.socrata.SocrataConfigurationSetting>
        </settings>
        <customTags/>
        <addRowNumber>true</addRowNumber>
      </com.axibase.collector.model.job.socrata.SocrataConfiguration>
    </configurations>
  </com.axibase.collector.model.job.socrata.SocrataJobConfig>
</list>